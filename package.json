{
  "name": "done",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:zero-cache": "zero-cache-dev -p src/schema.ts",
    "dev:db-up": "docker compose --env-file .env -f ./docker/docker-compose.yml up",
    "dev:db-down": "docker compose --env-file .env -f ./docker/docker-compose.yml down",
    "dev:clean": "source .env && docker volume rm -f docker_done_pgdata && rm -rf \"${ZERO_REPLICA_FILE}\"*",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:diff": "atlas schema diff --from 'postgresql://user:password@127.0.0.1:5430/done?sslmode=disable' --to file://atlas/schema.hcl --dev-url docker://postgres/16",
    "db:apply": "atlas schema apply --url 'postgresql://user:password@127.0.0.1:5430/done?sslmode=disable' --to file://atlas/schema.hcl --dev-url docker://postgres/16",
    "db:inspect": "atlas schema inspect --url 'postgresql://user:password@127.0.0.1:5430/done?sslmode=disable' --dev-url docker://postgres/16 > ./atlas/schema.hcl"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tooltip": "^1.1.6",
    "@rocicorp/zero": "^0.10.2024122404",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookies-next": "^5.0.2",
    "jose": "^5.9.6",
    "lucide-react": "^0.469.0",
    "mobx": "^6.13.5",
    "mobx-persist-store": "^1.1.5",
    "next": "15.1.3",
    "postgres": "^3.4.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
